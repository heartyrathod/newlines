}

function ViewMore() {
  
  jQuery(".view-more-box").slice(0, 3).show();  
  jQuery("#view-more-btn").click(function(e) {
    e.preventDefault();  
    jQuery(".view-more-box:hidden").slice(0, 3).fadeIn("slow");
    if (jQuery(".view-more-box:hidden").length == 0) {
      jQuery("#view-more-btn").fadeOut("slow");
      
    }
  });
  

}

function popUp() {
  jQuery('.pop-btn').on('click', function() {
      jQuery('.lightbox').fadeIn(200);
      jQuery('.lightbox-content').fadeIn(200); 
  });

  jQuery('.lightbox').on('click', function(event) {
      if (event.target === this) {
          jQuery('.lightbox').fadeOut(200); 
          jQuery('.lightbox-content').fadeOut(200); 
      }
  });

  jQuery('.close-btn').on('click', function() {
      jQuery('.lightbox').fadeOut(200); 
      jQuery('.lightbox-content').fadeOut(200); 
  });

}

function listactive() {
  var links = document.querySelectorAll('.elementor-icon-list-item a');
 
  function setActiveClass(event) {
      
      links.forEach(function(link) {
          link.classList.remove('active');
      });

      event.currentTarget.classList.add('active');
  }
  links.forEach(function(link) {
      link.addEventListener('click', setActiveClass);
  });

  function handleHashChange() {
      var currentHash = window.location.hash;
      links.forEach(function(link) {
          if (link.getAttribute('href') === currentHash) {
              link.classList.add('active');
          } else {
              link.classList.remove('active');
          }
      });
  }
  handleHashChange();
  window.addEventListener('hashchange', handleHashChange);
}

// function smothScroll() {
// 	jQuery('.smothScroll, a[href^="#"]').click(function() {
// 		var hedheight = jQuery('header.header').height();
// 		var targetDiv = jQuery(this).attr('href');
// 		var pos = jQuery(targetDiv).offset().top;
// 		jQuery('html, body').animate({
// 			scrollTop: pos - hedheight
// 		}, 1000);
// 		return false;		
// 	});
// }

function smothScroll() {
  jQuery('.smothScroll, a[href^="#"]').not('.nav-tabs a').click(function() {
      var hedheight = jQuery('header.header').height();
      var targetDiv = jQuery(this).attr('href');
      var pos = jQuery(targetDiv).offset().top;
      jQuery('html, body').animate({
          scrollTop: pos - hedheight
      }, 1000);
      return false;		
  });
}

function electricalSilder() {
 
  if(!jQuery('.electricalSilder').hasClass('slick-initialized')) {
      jQuery('.electricalSilder').slick({
              dots: true,
              arrows: false,
              infinite: false,
              autoplay:false,
              slidesToShow: 1,
              slidesToScroll: 1,
              adaptiveHeight: true,
              autoplaySpeed: 2000,
            
              responsive: [
              
              {
                breakpoint: 991,
                settings: {
                  slidesToShow: 2,
                  slidesToScroll: 2
                }
              },
              {
                breakpoint: 767,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1 
                  }
              }
          ]
      });
  }
}

function bodySlider() {
 
  if(!jQuery('.bodySlider').hasClass('slick-initialized')) {
      jQuery('.bodySlider').slick({
              dots: false,
              arrows: true,
              infinite: false,
              autoplay:false,
              slidesToShow: 3,
              slidesToScroll: 3,
              adaptiveHeight: true,
              autoplaySpeed: 2000,
            
              responsive: [
              
              {
                breakpoint: 991,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1
                }
              },
              {
                breakpoint: 767,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1 
                  }
              }
          ]
      });
  }
}

function modernSilder() {
 
  if(!jQuery('.modernSilder').hasClass('slick-initialized')) {
      jQuery('.modernSilder').slick({
              dots: true,
              arrows: false,
              infinite: false,
              autoplay:false,
              slidesToShow: 1,
              slidesToScroll: 1,
              adaptiveHeight: true,
              autoplaySpeed: 2000,
            
              responsive: [
              
              {
                breakpoint: 991,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 2
                }
              },
              {
                breakpoint: 767,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1 
                  }
              }
          ]
      });
  }
}

function accoPage() {
   // Remove any existing click handlers to prevent duplicates
   jQuery(".accordion__header").off("click").on("click", function (e) {
    e.preventDefault(); // Prevent default action

    var jthis = jQuery(this);
    var $accordionBody = jthis.next(".accordion__body"); // Use jthis here

    // Toggle the active state and slide up/down
    if (jthis.hasClass("active")) {
        // Close the active section
        $accordionBody.slideUp(() => {
            jthis.removeClass("active");
        });
    } else {
        // Close all other sections
        jQuery(".accordion__body").slideUp();
        jQuery(".accordion__header").removeClass("active");

        // Open the clicked section
        $accordionBody.slideDown(() => {
            jthis.addClass("active");
        });
    }
});
  }
 
  function gallerySlider() {
  function initSlickSliders(tabId) {
      const sliderFor = `${tabId} .slider-for`;
      const sliderNav = `${tabId} .slider-nav`;

      // Destroy existing instances if they exist
      if (jQuery(sliderFor).hasClass('slick-initialized')) {
          jQuery(sliderFor).slick('unslick');
      }
      if (jQuery(sliderNav).hasClass('slick-initialized')) {
          jQuery(sliderNav).slick('unslick');
      }

      // Initialize the sliders
      jQuery(sliderFor).slick({
          slidesToShow: 1,
          slidesToScroll: 1,
          arrows: true,
          fade: true,
          asNavFor: sliderNav
      });

      jQuery(sliderNav).slick({
          slidesToShow: 3,
          slidesToScroll: 1,
          asNavFor: sliderFor,
          dots: true,          
          focusOnSelect: true
      });
  }

  // Initialize the first tab's sliders
  initSlickSliders('#nav-exterior');

  // On tab change, initialize the respective sliders
  jQuery('.nav-tabs a').on('click', function(e) {
      e.preventDefault(); // Prevent default anchor behavior
      const tabId = jQuery(this).attr('href');
      
      // Activate the clicked tab
      jQuery('.nav-tabs .nav-link').removeClass('active');
      jQuery(this).addClass('active');

      // Hide all tab content and show the selected one
      jQuery('.tab-pane').removeClass('show active');
      jQuery(tabId).addClass('show active');

      // Initialize the sliders for the newly activated tab
      initSlickSliders(tabId);
  });

}

function buttonClick() {
  const formButton = document.getElementById('formButton');
  const noTradeButton = document.getElementById('noTradeButton');
  const form = document.getElementById('form1');

  formButton.addEventListener('click', function() {
      form.style.display = 'block'; // Show the form
  });

  noTradeButton.addEventListener('click', function() {
      form.style.display = 'none'; // Hide the form
  });
  
}



