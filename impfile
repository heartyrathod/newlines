function tabinslider() {
  var slideCount = jQuery('.hybrid .hybrid_box').length;

  if (jQuery('.hybrid').hasClass('slick-initialized')) {
      jQuery('.hybrid').slick('unslick');
  }

  if (slideCount > 0) {
      let settings = {
          dots: false,
          arrows: false,
          infinite: false,
          autoplay: true,
          slidesToShow: (slideCount > 4) ? 4 : slideCount,
          slidesToScroll: (slideCount > 4) ? 4 : slideCount,
          adaptiveHeight: true,
          autoplaySpeed: 2000,
          responsive: [
              {
                  breakpoint: 991,
                  settings: {
                      slidesToShow: (slideCount >= 2) ? 2 : slideCount,
                      slidesToScroll: (slideCount >= 2) ? 2 : slideCount
                  }
              },
              {
                  breakpoint: 767,
                  settings: {
                      slidesToShow: 1,
                      slidesToScroll: 1
                  }
              }
          ]
      };
      
      jQuery('.hybrid').slick(settings);
  }
}




