function boxsilder() {
 
  if(!jQuery('.choose').hasClass('slick-initialized')) {
      jQuery('.choose').slick({
              dots: false,
              arrows: true,
              infinite: false,
              autoplay:false,
              slidesToShow: 4,
              slidesToScroll: 4,
              adaptiveHeight: true,
              autoplaySpeed: 2000,
            
              responsive: [
              
              {
                breakpoint: 991,
                settings: {
                  slidesToShow: 2,
                  slidesToScroll: 2
                }
              },
              {
                breakpoint: 767,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1 
                  }
              }
          ]
      });
  }
}

function reviewsilder() {
 
  if(!jQuery('.main_review .e-con-inner').hasClass('slick-initialized')) {
      jQuery('.main_review .e-con-inner').slick({
              dots: false,
              arrows: true,
              infinite: false,
              autoplay:true,
              slidesToShow: 3,
              slidesToScroll: 3,
              adaptiveHeight: true,
              autoplaySpeed: 2000,
            
              responsive: [                
              {
                  breakpoint: 9999, 
                  settings: 'unslick'
              },
              {
                breakpoint: 991,
                settings: {
                  slidesToShow: 2,
                  slidesToScroll: 2
                }
              },
              {
                breakpoint: 767,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1 
                  }
              }
          ]
      });
  }
}

